<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.canvas.Canvas?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ironpanthers.scouting.desktop.ScoutingController"
            fx:id="root"
            onKeyPressed="#handleOnKeyPressed"
            prefHeight="720" prefWidth="1280">

    <GridPane >
        <Canvas fx:id="robotTimeline"
                GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2"/>

        <FlowPane fx:id="targetButtons"
                  GridPane.columnIndex="1" GridPane.rowIndex="1"/>

        <VBox fx:id="endgameStates"
              GridPane.columnIndex="2" GridPane.rowIndex="1"/>
    </GridPane>

</AnchorPane>
